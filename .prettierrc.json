{
    "$schema": "https://json.schemastore.org/prettierrc",
    "printWidth": 150,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "singleAttributePerLine": true,
    "vueIndentScriptAndStyle": true
}
